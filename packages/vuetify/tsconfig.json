{
  "extends": "../../tsconfig.json",
  "include": [
    "src",
    "dev"
  ],
  "exclude": ["types-temp"],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./types-temp",
    "paths": {
      "vuetify": ["src/entry-bundler.ts"],
      "@/*": [
        "src/*"
      ],
      "types": [
        "jest",
        "node",
        "vue-router"
      ]
    },
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  }
}
